
public class Gamer {
	void create() {
		System.out.println("Making game!");

		DiscManager dm = new DiscManager();
		Disc small = dm.getSmallDisc();

		Tower t1 = new Tower();
		t1.addDisc(small);
		t1.draw();

	}

	void move(int from, int to) {
		System.out.println("Moving " + from + " to " + to);
	}
}
